/* This stylesheet generated by Transfonter (http://transfonter.org) */

@font-face {
	font-family: 'Candara';
	src: url('../fonts/Candara.eot');
	src: url('../fonts/Candara.eot?#iefix') format('embedded-opentype'),
		url('../fonts/Candara.woff') format('woff'),
		url('../fonts/Candara.ttf')  format('truetype');
	font-weight: normal;
	font-style: normal;
}
a{
	-webkit-transition:all .3s;
	-o-transition:all .3s;
	-moz-transition:all .3s;
}
.main_content{margin-top: 50px; float: left; width: 100%;}
.padding{padding: 0 !important;}
.img img{width: 100%;}
.main_wrapper{float: left; width: 100%;}
.go_search > a {font-family: 'Candara'; background: none repeat scroll 
0 0 #026a91; border: 1px solid #00425b; border-radius: 3px; display: block; line-height: 35px; margin-left: 10px; text-align: center; width: 100%; color: #fff; font-size: 18px; font-weight: bold;}
.main_navigation{ display: block; background: #068ec1; float: left; width: 100%; line-height: 49px;}
.main_navigation ul{list-style: none; margin: 0; padding: 0;}
.main_navigation ul li a{ float: left;padding: 0px 20px;font-family: 'PT Sans', sans-serif; color: #fff; font-weight: 700; font-size: 16px; line-height: 49px;}
.main_navigation ul li a:hover{background-color: #026a91;}
.active{background-color: #026a91; color: #fff !important;}
.toggle_button{display: none; width: 30px; height: 38px; cursor: pointer; margin-left: 15px;}
.slider_bottom{float: left; width: 100%; background: #a3c3a7; height: 39px;}
.main_content h1{font-family: 'Candara'; font-weight: bold; font-size: 31px; color: #3d3d3d;}
.main_content h2{font-family: 'Candara'; font-weight: bold; font-size: 26px; color: #3d3d3d;}
.main_content p{ margin-bottom: 0.6rem; font-family: 'Candara'; font-weight: 100; font-size: 15px; color: #3d3d3d; text-align: justify;}
.conntent_right_img{margin-top: 60px;}
.footer_top{float: left; width: 100%; background: #b1e8fc; height: 159px; margin-top: 50px; }
.bottom_footer{background: none repeat scroll 0 0 #ccc;
    float: left;
    margin-top: 40px;
    width: 100%;}
.footer_links ul{list-style: none; margin: 0; padding: 0;}
.footer_links ul li{float: left; padding: 11px 0px;  font-family: 'Candara'; color: #242424;}
.footer_links ul li a{ padding: 0px 25px; border-right: solid 1px #ccc;  font-family: 'Candara'; color: #242424;}
.last_link a{border: none !important;}
.main_slider.img { float: left; width: 100%;}
.navigation_width{width: 100%; max-width: 995px; margin: 0 auto;}
/*=========sign-up-owner page css==============*/
.Heading h1{    margin: 0; padding: 0; color: #565656; background-color: #f3f3f3; display: block; border-radius: 5px 5px 0px 0px; padding: 4px 0px 10px 20px;}
.all_content{float: left; width: 100%; border: solid 3px #f3f3f3; padding: 30px;}
.all_content h2{color: #565656;}
.textarea_margin{margin-top: 132px;}
.textarea_margin textarea{height: 113px !important;}
.Submit_button{background-color: #026a91; display: block; width: 180px; text-align: center; line-height: 42px; border-radius: 3px; color: #fff; font-size: 19px;}
.Submit_button:hover{color: #fff; background-color: #025979;}

/*=========4 my-account-Parent page css==============*/
.Profile_information{background-color: #068ec1; color: #fff; line-height: 38px; font-weight: bold;}
.profile_back{background-color:#ededed; border: solid 1px #ababab; border-radius: 3px; float: left !important; width: 100%; margin-bottom: 33px;}
.Profile_img{border-right: solid 1px #ababab; padding: 18px 11px;}
.profile_info{border-right: solid 1px #ababab; border-bottom: solid 1px #ababab; line-height: 30px; font-size: 13px;}
.profilefull_info{ border-bottom: solid 1px #ababab; line-height: 30px; font-size: 13px;}

/*=========my-account-district-code page css==============*/
.full_width{width: 100% !important;}
.District_textarea textarea{height: 113px !important;}
.tab_nav{background-color: #ededed; padding-left: 30px; margin-bottom: 10px; height: 40px;}
.tab_nav ul{list-style: none; margin: 0; padding: 0; }
.tab_nav ul li{display: inline-block; margin-left: -4px;}
.tab_nav ul li a{color: #565656; padding: 11px 30px; line-height: 40px;}
.tab_nav ul li a:hover{background-color: #026a91; color: #fff;}


/*=========my-account-district-school page css==============*/
table thead{background: #068EC1 !important;}
table thead tr th, table thead tr td { color: #fff !important;}
table thead tr th, table tfoot tr th, table tfoot tr td, table tbody tr th, table tbody tr td, table tr td{ width: 100%; font-weight: bold;     border: 1px solid #000;}
.main_table{width: 100%; border: solid 1px #080808; overflow-y: hidden;}
table tr.even, table tr.alt, table tr:nth-of-type(even) { background: #c6f0ff;}
.checkbox{width: 50px; text-align: center;}
.body_name, .school_name, .Actions { width: 100%;}
.checkbox input[type="checkbox"]{margin: 0 !important;}
.actions{width: 130px; display: block;}
.actions2{width: 200px; display: block;}
.actions span a, .actions2 span a{color: #000; font-weight: bold;}


/*=========school page css==============*/
.midle_social_icon ul{list-style: none; margin: 0; padding: 0;}
.midle_social_icon ul li a {
float: left;
width: 33px;
text-align: center;
background-color: #464646;
margin: 1px 4px;
color: #fff;
padding: 8px 0;
}
.midle_social_icon ul li a:hover{background-color: #026a91; margin-top: -5px;}
.shadow {
margin-top: -1px;
float: left;
width: 100%;
position: relative;
z-index: -9;
}
.shadow img { width: 92%;}
address{font-size: 13px; font-style: normal; color: #8c8c8c;}


/*=========11 Company Listting page css==============*/
.Employe_Name{display: block !important; width: 130px !important;}
.date_icon{background-image: url("../images/date-icon.jpg");
    background-position: 97% 45%;
    background-repeat: no-repeat;}
.date_icon:focus{background-image: url("../images/date-icon.jpg") !important;
    background-position: 97% 45% !important;
    background-repeat: no-repeat !important;}
.time_icon{background-image: url("../images/time-icon.jpg");
    background-position: 97% 45%;
    background-repeat: no-repeat;}
.time_icon:focus{background-image: url("../images/time-icon.jpg") !important;
    background-position: 97% 45% !important;
    background-repeat: no-repeat !important;}


/*=========Beacons-Information page css==============*/
.beacons_content{float: left; width: 100%; margin-top: 50px;}
.beacons_content p{font-size: 17px !important; color: #5a5a5a !important;}
.bottom_services{margin: 0; padding: 0; margin-top: 20px;}
.bottom_services li{list-style: none; margin: 0; padding: 0; font-weight: bold; color: #5a5a5a;
line-height: 32px;}
/*==============flash message css=========*/
  div#flashMessage {
width: 100%;
float: left;
text-align: center;
margin: 10px 0px;
color: red;
overflow: hidden;
background: #BAEBFD;
padding: 5px 0;
position: absolute;
top: 124px;
}
/*==============Auth Flash message css=========*/
  div#authMessage {
width: 100%;
float: left;
text-align: center;
margin: 10px 0px;
color: red;
overflow: hidden;
background: #BAEBFD;
padding: 5px 0;
position: absolute;
top: 124px;
}

  .next, .prev, .first > a {
    margin: 5px;
}
.asc, .desc {
  color: #fff;
}

/*====error message css====*/
.error-message,.star{
color:red;
}


/*=========Beacons-listing page css==============*/
.actions{text-align: center; width: 200px;}
.actions_arrow{text-align: center; padding: 2px;}


/*=========Beacons user page css==============*/
.beacons_user_services{margin: 0; padding: 0; margin-top: 20px;}
.beacons_user_services li{color: #5a5a5a; font-size: 13px; margin-left:12px; }

@media screen and (max-width: 667px){
	.shadow{display: none;}
.main_table{overflow-y: hidden;}
.tab_nav{background-color: #ededed; padding-left: 5px; margin-bottom: 10px; height: auto;}
.tab_nav ul li{display: block; margin-left: -4px;}
.tab_nav ul li a{color: #565656; padding: 11px 20px; line-height: 20px; display: block;}
	.main_content{margin-top: 10px;}
	.conntent_right_img{margin-top: 0px;}
	.toggle_button span{float: left; width: 100%; background: #026a91; height: 4px;margin: 2px 0px;}
	.toggle_button{display: block;}
	.main_navigation {display: none;}
	.main_navigation ul li a{display: block; float: none; text-align: center; line-height: 38px; border-bottom: 1px solid #005f82;}
}
@media screen and (max-width: 320px){
.actions, .actions2{width: 100%; line-height: 1;}
table thead tr th, table tfoot tr th, table tfoot tr td, table tbody tr th, table tbody tr td, table tr td{    font-size: 10px; line-height: 0;}
}








